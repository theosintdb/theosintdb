{
  "name": "gosid---the-global-open-source-intelligence-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "npm --prefix server install && (test -f server/.env || cp server/.env.example server/.env) && npm --prefix server run prisma:generate && npm --prefix server run prisma:migrate || true && npm --prefix server run prisma:seed || true",
    "server:dev": "npm --prefix server run dev",
    "dev": "concurrently -n server,web -c yellow,cyan \"npm:server:dev\" \"vite\"",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "6.22.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "concurrently": "^9.1.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
